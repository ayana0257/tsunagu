<h2>Trying to spread!</h2>

<%= form_for(@blog) do |f|%>
	<h4>title</h4>
	  <%= f.text_field :title %>
	<h4>picture</h4>
	  <%= f.attachment_field :blog_images_images, multiple: true %>
	<h4>text</h4>
	  <%= f.text_field :body %>

	  <%= f.submit "投稿" %>
<% end %>

<%= link_to "戻る", root_path %>

