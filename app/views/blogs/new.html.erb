
<h2>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Trying to spread!</h2>


<div class="container">
  <div class="row">
    <div class="col-lg-4">
<%= form_for(@blog) do |f|%>
	<h3>title</h3>
	  <%= f.text_field :title %>
	<h3>picture</h3><h5>(＊複数枚選択できます)</h5>
	  <%= f.attachment_field :blog_images_images, multiple: true %>
	<h3>text</h3>
	  <%= f.text_field :body %>&nbsp;&nbsp;&nbsp;&nbsp;<%= f.submit "投稿する",class: "btn btn-warning" %>
<% end %>
</div>
</div>
</div>


&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= link_to "戻る", root_path, class: "btn btn-link" %>

