
<h4>title</h4>
  <%= @blog.title %>
  </br>
  <% if @blog.blog_images.present? %>
	  <% @blog.blog_images.each do |image| %>
	    <%= attachment_image_tag image, :image, :fill, 200, 200 %>
	  <% end %>
	  <% else %>
	  画像無し
  <% end %>
  </br><%= @blog.body %>

  <%= link_to "edit", edit_blog_path %>
  <%= link_to "削除", destroy_blog_path(@blog.id), method: :delete, "data-confirm" => "本当に削除しますか？" %>

  <%= link_to "戻る", blogs_path %>